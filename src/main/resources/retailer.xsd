<?xml version="1.0"?>

<xsd:schema version="1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.itemmaster.com/feed/item" targetNamespace="http://www.itemmaster.com/feed/item" elementFormDefault="qualified">

    <xsd:element name="items">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="item" type="FeedItemType" minOccurs="0" maxOccurs="unbounded" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="FeedItemType">
        <xsd:sequence>
            <xsd:element name="companyTrackingId" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                            <xsd:attribute name="id" type="xsd:string" use="required" />
                            <xsd:attribute name="name" type="xsd:string" use="required" />
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ecommerceTitle">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ecommerceDescription" type="LocalizedStringType">
                <xsd:annotation>
                    <xsd:documentation>
                        Factual prose product description that describes the item.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="frontOfPackageText" type="xsd:string" minOccurs="0" />
            <xsd:element name="restOfPackageText" type="xsd:string" minOccurs="0" />
            <xsd:element ref="productCodes" />
            <xsd:element name="manufacturerId">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string" >
                        <xsd:minLength value="36" />
                        <xsd:maxLength value="36" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="categoryId">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string" >
                        <xsd:minLength value="2" />
                        <xsd:maxLength value="16" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="privateLabelItem" type="YesNoIndicatorType" minOccurs="0" default="N"/>
            <xsd:element name="distributorId" type="xsd:string" minOccurs="0" />
            <xsd:element name="importerId" type="xsd:string" minOccurs="0" />
            <xsd:element ref="media" />
            <xsd:element ref="packageData" />
            <xsd:element ref="planogramData" minOccurs="0" />
            <xsd:element name="packageManufacturer" type="CompanyDetails" minOccurs="0" />
            <xsd:element name="packageDistributor" type="CompanyDetails" minOccurs="0" />
            <xsd:element name="packageImporter" type="CompanyDetails" minOccurs="0" />
            <xsd:element name="contentPlus" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="images" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="logos" type="CustomMedia" minOccurs="0" maxOccurs="unbounded" />
                        <xsd:element name="videos" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="mobile" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="other" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="sellCopy" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="ecommerceDescription" type="LocalizedStringType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>
                                    Manufacturer supplied e-commerce description
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="bulletPoints" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="bulletPoint" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="manufacturerContent" type="manufacturerContent"/>
            <xsd:element ref="products" minOccurs="0" />
            <xsd:element ref="attributes" minOccurs="0" />
        </xsd:sequence>
        <xsd:attribute name="type" type="ProductType" use="required" />
    </xsd:complexType>

    <xsd:complexType name="manufacturerContent">
        <xsd:sequence>
            <!-- <xsd:element minOccurs="0" name="default">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="unqualified" name="contentPlus" type="ContentPlus"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element> -->
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="retailers" type="Retailer"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="contentPlus">
        <xsd:sequence>
            <xsd:element name="images" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="logos" type="CustomMedia" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="videos" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="mobile" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="other" type="CustomMedia" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="sellCopy" type="xsd:string" minOccurs="0"/>
            <xsd:element name="ecommerceDescription" type="LocalizedStringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Manufacturer supplied e-commerce description
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="bulletPoints" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="bulletPoint" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Retailer">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="contentPlus" type="contentPlus"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required" />
    </xsd:complexType>

    <xsd:element name="productCodes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="productCode" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:simpleContent>
                            <xsd:extension base="xsd:string">
                                <xsd:attribute name="type" type="ProductCodeType" use="required" />
                            </xsd:extension>
                        </xsd:simpleContent>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="media">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="medium" minOccurs="1" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="url" type="xsd:anyURI" />
                            <xsd:element name="path" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>
                                        To maintain path of the downloaded image. Not for end user.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="view" type="ViewType" use="required" />
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="packageData">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="length" type="DimensionalMeasurementType" />
                <xsd:element name="height" type="DimensionalMeasurementType" />
                <xsd:element name="width" type="DimensionalMeasurementType" />
                <xsd:element name="weight" type="WeightMeasurementType" />
                <xsd:element name="unitsInPackage" type="xsd:int" />
                <xsd:element name="packageType" type="xsd:string" />
                <xsd:element name="packageSize" type="PackageMeasurementType" />
                <xsd:element name="dryWeight" type="WeightMeasurementType" minOccurs="0" />
                <xsd:element name="netWeight" type="WeightMeasurementType" minOccurs="0" />
                <xsd:element name="information" type="xsd:string" minOccurs="0" />
                <xsd:element name="recyclable" type="YesNoIndicatorType" minOccurs="0" />
            </xsd:sequence>
            <xsd:attribute name="type" type="PackageDataType" use="required" />
            <xsd:attribute name="approximation" type="YesNoIndicatorType" default="Y"/>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="planogramData">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="depthCount" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="widthCount" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="heightCount" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="depthNesting" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="verticalNesting" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="displayDepth" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="displayHeight" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="displayWidth" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="divisionName" type="xsd:string" minOccurs="0" />
                <xsd:element name="dualNesting" type="xsd:string" minOccurs="0" />
                <xsd:element name="multipleShelfFacings" type="xsd:integer" minOccurs="0" />
                <xsd:element name="pegDown" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="pegRight" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="trayCount" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="trayDepth" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="trayWidth" type="xsd:decimal" minOccurs="0" />
                <xsd:element name="trayHeight" type="xsd:decimal" minOccurs="0" />
            </xsd:sequence>
            <xsd:attribute name="shelfManFlag" type="YesNoIndicatorType" use="optional" />
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="attributes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="attribute" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="name" type="xsd:string" />
                            <xsd:element name="value" type="xsd:string" />
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="products">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="product" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="description" type="xsd:string" />
                            <xsd:element name="distributorId" type="xsd:string" minOccurs="0" />
                            <xsd:element name="importerId" type="xsd:string" minOccurs="0" />
                            <xsd:element name="brandId">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string" >
                                        <xsd:minLength value="36" />
                                        <xsd:maxLength value="36" />
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="subBrands" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="subBrand" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="brandDescription" type="xsd:string" minOccurs="0" />
                            <xsd:element name="secondaryBrandDescription" type="xsd:string" minOccurs="0" />
                            <xsd:element name="manufacturerId" type="xsd:string" minOccurs="0" />
                            <xsd:element name="line" type="xsd:string" minOccurs="0" />
                            <xsd:element name="variety" type="xsd:string" minOccurs="0" />
                            <xsd:element name="seasonal" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="countryOfManufacturing" type="xsd:string" minOccurs="0" />
                            <xsd:element name="warnings" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="warning" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="drugInteractions" type="xsd:string" minOccurs="0" />
                            <xsd:element name="directions" type="xsd:string" minOccurs="0" />
                            <xsd:element name="indications" type="xsd:string" minOccurs="0" />
                            <!--START ALTS-->
                            <xsd:element name="isAerosol" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="isChemical" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="isPesticide" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="hasBatteries" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="containsMercury" type="YesNoIndicatorType" minOccurs="0"/>
                            <xsd:element name="hasFuelContainer" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="containsPaperWood" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="compositeWoodCertificationCode" type="compositeWoodType"/>
                            <xsd:element name="hasExpiration" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="hasWarranty" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="isProp65WarningRequired" type="YesNoIndicatorType"/>
                            <xsd:element name="isTemperatureSensitive" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="smallPartsWarnings" type="smallPartWarningsType"/>
                            <xsd:element name="isControlledSubstance" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="hasStateRestrictions" type="YesNoIndicatorType" minOccurs="0"/>
                            <xsd:element name="isNutritionFactsLabelRequired" type="YesNoIndicatorType" minOccurs="0" />
                            <xsd:element name="isBoxTopsForEducationEligible" type="YesNoIndicatorType" minOccurs="0" />
                            <!--END ALTS-->
                            <xsd:element name="grocery" type="GroceryType" minOccurs="0" />
                        </xsd:sequence>
                        <xsd:attribute name="type" type="ProductType" use="required" />
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="GroceryType">
        <xsd:sequence>
            <xsd:element name="ndcCode" type="xsd:string" minOccurs="0" />
            <xsd:element name="npnCode" type="xsd:string" minOccurs="0" />
            <xsd:element name="dinCode" type="xsd:string" minOccurs="0" />
            <xsd:element name="alcoholByVolumePercent" type="xsd:string" minOccurs="0" />
            <xsd:element name="alcoholType" type="xsd:string" minOccurs="0" />
            <xsd:element name="ingredients" type="xsd:string" minOccurs="0" />
            <xsd:element name="vitaminsAndMinerals" type="xsd:string" minOccurs="0" />
            <xsd:element name="nutritionalClaims" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="fatFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="goodSourceOfFiber" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="lowFat" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="lowSodium" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="sugarFree" type="YesNoIndicatorType" minOccurs="0" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="kosherCodes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="kosherCode" type="xsd:int" minOccurs="0" maxOccurs="unbounded" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="containerRefunds" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="containerRefund" minOccurs="1" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="locale" type="xsd:string" />
                                    <xsd:element name="amount" type="xsd:string" />
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="certifications" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="certificate" type="xsd:int" minOccurs="0" maxOccurs="unbounded" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="recipes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="recipe" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="foodRelatedIndicators" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="vegan" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="vegetarian" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="lactoseFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="flavor" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="antibioticFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="temperatureIndicator" type="TemperatureType" />
                        <xsd:element name="wheatFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="glutenFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="hormoneFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="natural" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="nitratesFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="nitritesFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="organic" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="peanutFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="readyToCook" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="readyToHeat" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="dairyFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="eggFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="nonGmo" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="kosher" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="shellfishFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="soyFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="milkFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="crustaceanFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="fishFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="sesameFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="nutFree" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="halal" type="YesNoIndicatorType" minOccurs="0" />
                        <xsd:element name="paleo" type="YesNoIndicatorType" minOccurs="0" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="recycleCodes" type="xsd:string" minOccurs="0" />
            <xsd:element name="nutritions" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="nutrition" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="title" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="numberServedInPackage" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="numberOfServings" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="servingSizes" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="servingSize" minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:complexType>
                                                        <xsd:complexContent>
                                                            <xsd:extension base="MeasurementType">
                                                                <xsd:attribute name="type" type="MeasurementUomType" use="required" />
                                                            </xsd:extension>
                                                        </xsd:complexContent>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="servingSizeFullTxt" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="raccAmt" type="MeasurementType" minOccurs="0" />
                                    <xsd:element name="energy" type="xsd:integer" minOccurs="0" />
                                    <xsd:element name="totalFat" type="UomDailyPctCaloriesType" minOccurs="0" />
                                    <xsd:element name="saturatedFat" type="UomDailyPctCaloriesType" minOccurs="0" />
                                    <xsd:element name="polyunsaturatedFat" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="monounsaturatedFat" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="transFat" type="UomType" minOccurs="0" />
                                    <xsd:element name="cholesterol" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="sodium" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="potassium" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="carbohydrates" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="dietaryFiber" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="soluableFiber" type="UomType" minOccurs="0" />
                                    <xsd:element name="insoluableFiber" type="UomType" minOccurs="0" />
                                    <xsd:element name="sugars" type="UomType" minOccurs="0" />
                                    <xsd:element name="addedSugars" type="UomType" minOccurs="0" />
                                    <xsd:element name="sugarAlchohol" type="UomType" minOccurs="0" />
                                    <xsd:element name="otherCarbohydrates" type="UomType" minOccurs="0" />
                                    <xsd:element name="protein" type="UomDailyPctType" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminA" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminC" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfCalcium" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfIron" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfCopper" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfFolicAcid" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfIodine" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfMagnesium" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfNiacin" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfPhosphorous" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfRiboflavin" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfThiamin" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminB12" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminB6" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminD" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminE" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfVitaminZinc" type="xsd:string" minOccurs="0" />
                                    <xsd:element name="dailyPercentOfAddedSugars" type="xsd:string" minOccurs="0" />
                                </xsd:sequence>
                                <xsd:attribute name="sequence" type="xsd:int" use="required" />
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ProductType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="grocery" />
        </xsd:restriction>
    </xsd:simpleType>

